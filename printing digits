#include <stdio.h>

int main() {
    int number, digit;

    printf("Enter an integer: ");
    scanf("%d", &number);

    printf("Digits: ");

    // Extract and print each digit
    while (number != 0) {
        digit = number % 10;
        printf("%d ", digit);
        number /= 10;
    }

    printf("\n");

    return 0;
}
